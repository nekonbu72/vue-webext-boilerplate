{
  "name": "vue-webext-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "watch": "vue-cli-service build --watch --mode development",
    "open": "web-ext run --firefox \"C:\\Program Files\\Mozilla Firefox\\firefox.exe\" --source-dir ./addon/",
    "start": "npm-run-all -p watch open"
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@types/firefox-webext-browser": "^67.0.2",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "vue-template-compiler": "^2.6.10",
    "web-ext": "^3.1.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
